<%@ Page Title="Home Page" Language="C#" MasterPageFile="~/Site.Master" AutoEventWireup="true" CodeBehind="Default.aspx.cs" Inherits="BooksLibrary.Web._Default" %>

<asp:Content ID="BodyContent" ContentPlaceHolderID="MainContent" runat="server">
    <img width="1150" height="100" style="margin-bottom: 20px;" src="data:image/jpeg;base64," alt="Alternate Text" />
    <asp:ListView runat="server" ID="NewestBooksListVIew"
        ItemType="BooksLibrary.Models.Book"
        DataKeyNames="Id"
        SelectMethod="NewestBooksListVIew_GetData">
        <LayoutTemplate>
            <div class="panel panel-primary col-md-4 col-md-offset-1">
                <div class="panel-heading">
                    <h3 class="panel-title">Latest books</h3>
                </div>
                <div class="panel-body">
                    <table class="table table-striped table-hover ">
                        <tbody>
                            <tr id="itemplaceholder" runat="server">
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </LayoutTemplate>
        <ItemTemplate>
            <tr id="itemplaceholder" runat="server">
                <td><%#: Item.Title %></td>
                <td><%#: Item.Author.Name %></td>
                <td>
                    <asp:Button runat="server" CommandArgument="<%# Item.Id %>" ID="BookDetailsButton" OnClick="BookDetailsButton_Click" Text="Details" class="btn btn-default" />
                </td>
            </tr>
        </ItemTemplate>
    </asp:ListView>
    <asp:ListView runat="server" ID="TopAuthorsListView"
        ItemType="BooksLibrary.Models.Author"
        DataKeyNames="Id"
        SelectMethod="TopAuthorsListView_GetData">
        <LayoutTemplate>
            <div class="panel panel-primary col-md-4 col-md-offset-1">
                <div class="panel-heading">
                    <h3 class="panel-title">Top Authors</h3>
                </div>
                <div class="panel-body">
                    <table class="table table-striped table-hover ">
                        <tbody>
                            <tr id="itemplaceholder" runat="server">
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </LayoutTemplate>
        <ItemTemplate>
            <tr id="itemplaceholder" runat="server">
                <td><%#: Item.Name %></td>
                <td><%#: Item.Books.Count %></td>
                <td>
                    <asp:Button runat="server" CommandArgument="<%# Item.Id %>" ID="AuthorDetailsButton" OnClick="AuthorDetailsButton_Click" Text="Details" class="btn btn-default" />
                </td>
            </tr>
        </ItemTemplate>
    </asp:ListView>
    <div class="col-md-2 pull-right"></div>
</asp:Content>
